{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": [
		"specs",
		"fixtures",
		"factories",
		"e2e-env.ts",
		"global-setup.ts",
		"global-teardown.ts",
		"playwright.config.ts"
	]
}
